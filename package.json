{
  "name": "mvp-bd-client",
  "version": "1.0.0",
  "description": "Minimum viable product of Bulldog client",
  "main": "server/src/index.js",
  "devDependencies": {
    "@babel/preset-env": "^7.10.4",
    "@types/jest": "^26.0.5",
    "@types/node": "^14.0.23",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "eslint": "^7.5.0",
    "eslint-config-verit": "^2.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.1.0",
    "jest-ts-webcompat-resolver": "^1.0.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.3",
    "typescript": "^3.9.7"
